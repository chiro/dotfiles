escape ^Zz

zombie ^[

autodetach on
startup_message off
defscrollback 8192

defencoding utf-8
encoding utf-8 utf-8
utf8 on on

vbell off
bell_msg "^G"
shell -$SHELL
defflow off

defbce on
term "screen-256color"
termcapinfo xterm* 'Co#256:pa#256:AB=\E[48;5;%dm:AF=\E[38;5;%dm:'

multiuser on
acladd $USER

bind l windowlist

caption always "%{=r dd}%-Lw%40L>%?%F%{=b dW}%:[%?%n%f %t%?%F%{-}%:]%?%+Lw"
hardstatus alwayslastline "%H%{-}%=[%l] %y/%m/%d %02c:%s"

sorendition "+rb .G"
